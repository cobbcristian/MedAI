# Enhanced AI Telemedicine Environment Configuration
# Copy this file to .env and update with your actual values

# =============================================================================
# DATABASE CONFIGURATION
# =============================================================================
MYSQL_ROOT_PASSWORD=your_secure_root_password_here
MYSQL_USER=ai_telemedicine_user
MYSQL_PASSWORD=your_secure_database_password_here
DATABASE_URL=jdbc:mysql://mysql:3306/ai_telemedicine?useSSL=false&allowPublicKeyRetrieval=true&serverTimezone=UTC

# =============================================================================
# AI & MACHINE LEARNING CONFIGURATION
# =============================================================================
OPENAI_API_KEY=your_openai_api_key_here
HUGGINGFACE_API_KEY=your_huggingface_api_key_here
GOOGLE_CLOUD_API_KEY=your_google_cloud_api_key_here

# AI Model Configuration
AI_MODEL_CACHE_DIR=/app/models
AI_MODEL_DOWNLOAD_ENABLED=true
AI_MODEL_UPDATE_FREQUENCY=weekly
AI_MODEL_VERSION_TRACKING=true

# Voice Analysis Configuration
VOICE_ANALYSIS_ENABLED=true
VOICE_RECORDING_RETENTION_DAYS=90
VOICE_ANALYSIS_MODEL=superb/wav2vec2-base-superb-ks
VOICE_PROCESSING_MODEL=facebook/wav2vec2-base-960h

# Predictive Analytics Configuration
PREDICTIVE_ANALYTICS_ENABLED=true
PREDICTION_MODEL_UPDATE_FREQUENCY=monthly
PREDICTION_CONFIDENCE_THRESHOLD=0.75
PREDICTION_RETENTION_DAYS=1825

# =============================================================================
# SECURITY & COMPLIANCE CONFIGURATION
# =============================================================================
JWT_SECRET=your_very_long_and_secure_jwt_secret_key_here
ENCRYPTION_KEY=your_base64_encoded_encryption_key_here
REDIS_PASSWORD=your_secure_redis_password_here

# HIPAA Compliance Settings
HIPAA_COMPLIANCE_MODE=true
AUDIT_LOGGING_ENABLED=true
DATA_ENCRYPTION_ENABLED=true
ACCESS_CONTROL_ENABLED=true
DATA_BACKUP_REQUIRED=true

# Security Monitoring
SECURITY_MONITORING_ENABLED=true
SECURITY_ALERT_EMAIL=security@aitelemedicine.com
SECURITY_ALERT_WEBHOOK=https://your-webhook-url.com/security-alerts

# Data Retention Policies (in days)
PATIENT_PHI_RETENTION_DAYS=2555
AUDIT_LOG_RETENTION_DAYS=2555
VOICE_RECORDING_RETENTION_DAYS=90
AI_ANALYSIS_RETENTION_DAYS=1825
SECURITY_EVENT_RETENTION_DAYS=2555
COMPLIANCE_REPORT_RETENTION_DAYS=3650

# =============================================================================
# MONITORING & OBSERVABILITY
# =============================================================================
PROMETHEUS_ENABLED=true
GRAFANA_ENABLED=true
ELASTICSEARCH_ENABLED=true
KIBANA_ENABLED=true
LOGSTASH_ENABLED=true

# Monitoring Passwords
GRAFANA_PASSWORD=your_secure_grafana_password_here
ELASTICSEARCH_PASSWORD=your_secure_elasticsearch_password_here

# =============================================================================
# CLOUD & DEPLOYMENT CONFIGURATION
# =============================================================================
ENVIRONMENT=production
REGION=us-east-1
AVAILABILITY_ZONE=us-east-1a

# Cloud Provider Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_REGION=us-east-1

# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT_ID=your_project_id_here
GOOGLE_CLOUD_CREDENTIALS_FILE=/path/to/credentials.json

# Azure Configuration
AZURE_SUBSCRIPTION_ID=your_subscription_id_here
AZURE_TENANT_ID=your_tenant_id_here
AZURE_CLIENT_ID=your_client_id_here
AZURE_CLIENT_SECRET=your_client_secret_here

# =============================================================================
# PERFORMANCE & SCALING CONFIGURATION
# =============================================================================
# Redis Configuration
REDIS_URL=redis://redis:6379
REDIS_MAX_CONNECTIONS=100
REDIS_CONNECTION_TIMEOUT=5000

# Database Connection Pool
DB_MAX_CONNECTIONS=50
DB_MIN_CONNECTIONS=10
DB_CONNECTION_TIMEOUT=30000
DB_IDLE_TIMEOUT=600000

# AI Model Performance
AI_MODEL_BATCH_SIZE=32
AI_MODEL_MAX_WORKERS=4
AI_MODEL_GPU_ENABLED=true
AI_MODEL_MEMORY_LIMIT=8GB

# =============================================================================
# INTEGRATION CONFIGURATION
# =============================================================================
# Email Service
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your_email@gmail.com
SMTP_PASSWORD=your_app_password_here
EMAIL_FROM=noreply@aitelemedicine.com

# SMS Service (Twilio)
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_PHONE_NUMBER=+1234567890

# Payment Processing (Stripe)
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here

# Video Calling (Twilio Video)
TWILIO_VIDEO_API_KEY=your_twilio_video_api_key_here
TWILIO_VIDEO_API_SECRET=your_twilio_video_api_secret_here

# =============================================================================
# FEATURE FLAGS
# =============================================================================
FEATURE_VOICE_ANALYSIS_ENABLED=true
FEATURE_PREDICTIVE_ANALYTICS_ENABLED=true
FEATURE_CLINICAL_DECISION_SUPPORT_ENABLED=true
FEATURE_BIAS_FAIRNESS_DASHBOARD_ENABLED=true
FEATURE_SECURITY_DASHBOARD_ENABLED=true
FEATURE_ADVANCED_AI_FEATURES_ENABLED=true

# =============================================================================
# LOGGING CONFIGURATION
# =============================================================================
LOG_LEVEL=INFO
LOG_FORMAT=json
LOG_RETENTION_DAYS=30
LOG_ROTATION_SIZE=100MB
LOG_ROTATION_COUNT=10

# Application Logging
APP_LOG_LEVEL=INFO
AI_LOG_LEVEL=DEBUG
SECURITY_LOG_LEVEL=WARN
AUDIT_LOG_LEVEL=INFO

# =============================================================================
# API CONFIGURATION
# =============================================================================
API_RATE_LIMIT_REQUESTS_PER_MINUTE=100
API_RATE_LIMIT_BURST_SIZE=20
API_TIMEOUT_SECONDS=30
API_MAX_REQUEST_SIZE=10MB

# CORS Configuration
CORS_ALLOWED_ORIGINS=https://your-domain.com,https://www.your-domain.com
CORS_ALLOWED_METHODS=GET,POST,PUT,DELETE,OPTIONS
CORS_ALLOWED_HEADERS=Content-Type,Authorization,X-Requested-With
CORS_ALLOW_CREDENTIALS=true

# =============================================================================
# HEALTH CHECK CONFIGURATION
# =============================================================================
HEALTH_CHECK_ENABLED=true
HEALTH_CHECK_INTERVAL_SECONDS=60
HEALTH_CHECK_TIMEOUT_SECONDS=10
HEALTH_CHECK_RETRY_COUNT=3

# =============================================================================
# BACKUP & DISASTER RECOVERY
# =============================================================================
BACKUP_ENABLED=true
BACKUP_SCHEDULE=0 2 * * *
BACKUP_RETENTION_DAYS=30
BACKUP_ENCRYPTION_ENABLED=true
BACKUP_COMPRESSION_ENABLED=true

# Disaster Recovery
DR_ENABLED=true
DR_REPLICATION_ENABLED=true
DR_FAILOVER_ENABLED=true
DR_RECOVERY_TIME_OBJECTIVE=4h
DR_RECOVERY_POINT_OBJECTIVE=1h

# =============================================================================
# DEVELOPMENT & TESTING
# =============================================================================
# Development Mode
DEV_MODE=false
DEBUG_MODE=false
TEST_MODE=false

# Testing Configuration
TEST_DATABASE_URL=jdbc:mysql://localhost:3306/ai_telemedicine_test
TEST_AI_MODELS_ENABLED=false
TEST_SECURITY_CHECKS_ENABLED=true

# =============================================================================
# THIRD-PARTY INTEGRATIONS
# =============================================================================
# FHIR Integration
FHIR_SERVER_URL=https://your-fhir-server.com
FHIR_CLIENT_ID=your_fhir_client_id
FHIR_CLIENT_SECRET=your_fhir_client_secret

# HL7 Integration
HL7_ENABLED=true
HL7_SERVER_PORT=8081
HL7_MESSAGE_LOGGING=true

# DICOM Integration
DICOM_ENABLED=true
DICOM_SERVER_PORT=4242
DICOM_STORAGE_PATH=/app/dicom

# =============================================================================
# CUSTOMIZATION & BRANDING
# =============================================================================
# Application Branding
APP_NAME=AI Telemedicine Platform
APP_VERSION=2.0.0
APP_DESCRIPTION=Advanced AI-powered telemedicine platform
APP_LOGO_URL=https://your-domain.com/logo.png
APP_FAVICON_URL=https://your-domain.com/favicon.ico

# UI Customization
UI_THEME=default
UI_PRIMARY_COLOR=#2196f3
UI_SECONDARY_COLOR=#f50057
UI_FONT_FAMILY=Roboto
UI_ENABLE_DARK_MODE=true

# =============================================================================
# COMPLIANCE & CERTIFICATION
# =============================================================================
# HIPAA Compliance
HIPAA_COMPLIANCE_CERTIFIED=true
HIPAA_COMPLIANCE_AUDIT_DATE=2024-01-15
HIPAA_COMPLIANCE_AUDITOR=Your Compliance Auditor

# SOC 2 Compliance
SOC2_COMPLIANCE_ENABLED=true
SOC2_TYPE=Type II
SOC2_AUDIT_DATE=2024-01-15

# GDPR Compliance
GDPR_COMPLIANCE_ENABLED=true
GDPR_DATA_PROCESSOR=true
GDPR_DATA_CONTROLLER=true

# =============================================================================
# PERFORMANCE TUNING
# =============================================================================
# JVM Settings for Java Backend
JVM_XMS=512m
JVM_XMX=2g
JVM_XX_MAXMETASPACESIZE=256m
JVM_XX GCTIMERATIO=4
JVM_XX GCTIMERATIO=4

# Python Settings for AI Backend
PYTHON_MAX_WORKERS=4
PYTHON_WORKER_TIMEOUT=30
PYTHON_MEMORY_LIMIT=4GB

# Node.js Settings for Frontend
NODE_ENV=production
NODE_OPTIONS=--max-old-space-size=2048

# =============================================================================
# SECURITY HARDENING
# =============================================================================
# SSL/TLS Configuration
SSL_ENABLED=true
SSL_CERTIFICATE_PATH=/etc/nginx/ssl/cert.pem
SSL_PRIVATE_KEY_PATH=/etc/nginx/ssl/key.pem
SSL_CIPHER_SUITE=TLS_AES_256_GCM_SHA384:TLS_CHACHA20_POLY1305_SHA256

# Security Headers
SECURITY_HEADERS_ENABLED=true
SECURITY_HEADERS_HSTS_MAX_AGE=31536000
SECURITY_HEADERS_CONTENT_SECURITY_POLICY=default-src 'self'
SECURITY_HEADERS_X_FRAME_OPTIONS=DENY
SECURITY_HEADERS_X_CONTENT_TYPE_OPTIONS=nosniff

# =============================================================================
# MONITORING ALERTS
# =============================================================================
# Alert Configuration
ALERT_EMAIL_ENABLED=true
ALERT_SMS_ENABLED=true
ALERT_WEBHOOK_ENABLED=true

# Alert Thresholds
ALERT_CPU_THRESHOLD=80
ALERT_MEMORY_THRESHOLD=85
ALERT_DISK_THRESHOLD=90
ALERT_RESPONSE_TIME_THRESHOLD=5000
ALERT_ERROR_RATE_THRESHOLD=5

# =============================================================================
# DEPLOYMENT CONFIGURATION
# =============================================================================
# Docker Configuration
DOCKER_REGISTRY=your-registry.com
DOCKER_IMAGE_TAG=latest
DOCKER_BUILD_CONTEXT=.

# Kubernetes Configuration
KUBERNETES_ENABLED=false
KUBERNETES_NAMESPACE=ai-telemedicine
KUBERNETES_REPLICAS=3

# Auto-scaling Configuration
AUTOSCALING_ENABLED=true
AUTOSCALING_MIN_REPLICAS=2
AUTOSCALING_MAX_REPLICAS=10
AUTOSCALING_TARGET_CPU_UTILIZATION=70 